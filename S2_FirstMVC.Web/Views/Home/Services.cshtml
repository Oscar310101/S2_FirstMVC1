@{ 
   List <Servicio> listado = ViewBag.ListadoServicio;
}


<h1>Mis Servicios Profesionales</h1>


<table class="table table-dark">
    <thead>
        <tr>
            <th>ID</th>
            <th>Descripción</th>
        </tr>


    </thead>
    <tbody>
        @foreach(var item in listado)
            {
                <tr>
                    <td>@item.id</td>
                    <td>@item.descripcion</td>
                </tr>
            }
    </tbody>



</table>